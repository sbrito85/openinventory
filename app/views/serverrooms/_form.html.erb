<%= simple_form_for(@serverroom, html: {class: "form-horizontal"}) do |f| %>
  <%= f.error_notification %>


    <%= f.input :name %>
    <%= f.input :office_id, :selected => session[:id], :collection => Office.order(:office), :label_method => :office %>


  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
